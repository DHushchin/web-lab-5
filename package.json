{
  "name": "HasuraLabs",
  "version": "1.0.0",
  "description": "This directory is a brief example of a [Svelte](https://svelte.dev/) site that can be deployed with Vercel and zero-configuration. If you need to use API Routes, we recommend using our [SvelteKit example](https://github.com/vercel/vercel/tree/main/examples/sveltekit).",
  "main": "rollup.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "husky install",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore",
    "eslint:check": "eslint .",
    "format:fix": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "stylelint:check": "stylelint \"src/**/*.{css,scss,svelte}\" --ignore-path .gitignore",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,svelte}\" --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DHushchin/HasuraLabs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DHushchin/HasuraLabs/issues"
  },
  "homepage": "https://github.com/DHushchin/HasuraLabs#readme",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.3",
    "dotenv": "^10.0.0",
    "graphql": "^16.2.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.1",
    "eslint": "^8.5.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.62.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0",
    "svelte": "^3.44.3"
  }
